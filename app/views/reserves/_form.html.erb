<%= form_with(model: reserve, local: true) do |form| %>
  <%= form_errors_for form.object  %>

  <%= form.label :initials %>
  <%= form.text_field :initials, id: :reserve_initials %>

  <%= form.label :description %>
  <%= form.text_field :description, id: :reserve_description %>

  <div class="actions">
    <%= link_to 'Voltar', reserves_path, :class => "btn" %>
    <%= form.submit 'Enviar', :class => "btn success" %>
  </div>
<% end %>

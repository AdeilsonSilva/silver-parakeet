<% content_for :title, "Munições" %>
<% unless notice.blank? %>
    <div class="info padding" >
        <p id="notice"><%= notice %></p>
    </div>
<% end %>

<div class="off-xs-3 xs-6 text-center">
    <div class="padding">
        <p>
          <strong>Caliber:</strong>
          <%= @ammunition.caliber %>
        </p>

        <p>
          <strong>Description:</strong>
          <%= @ammunition.description %>
        </p>
    </div>
</div>
<%= link_to 'Voltar', ammunitions_path, :class => "btn"  %>
<%= link_to 'Editar', edit_ammunition_path(@ammunition), :class => "btn success"  %>

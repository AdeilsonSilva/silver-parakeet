<p id="notice"><%= notice %></p>

<h1>Material Reserve Has Accessories</h1>

<table>
  <thead>
    <tr>
      <th>Material reserve</th>
      <th>Accessories</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="text-center">
    <% @material_reserve_has_accessories.each do |material_reserve_has_accessory| %>
      <tr>
        <td><%= material_reserve_has_accessory.material_reserve %></td>
        <td><%= material_reserve_has_accessory.accessories %></td>
        <td><%= material_reserve_has_accessory.amount %></td>
        <td><%= link_to 'Show', material_reserve_has_accessory %></td>
        <td><%= link_to 'Edit', edit_material_reserve_has_accessory_path(material_reserve_has_accessory) %></td>
        <td><%= link_to 'Destroy', material_reserve_has_accessory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


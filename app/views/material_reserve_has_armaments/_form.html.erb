<%= form_for [:reserf, @material_reserve, @material_reserve_has_armament] do |form| %>
  <%= form_errors_for form.object  %>

  <div class="field">
    <%= form.label :armaments_id %>
    <%= form.text_field :armaments_id, id: :material_reserve_has_armament_armaments_id %>
  </div>

  <div class="field">
    <%= form.label :material_reserve_id %>
    <%= form.text_field :material_reserve_id, id: :material_reserve_has_armament_material_reserve_id %>
  </div>

  <div class="field">
    <%= form.label :amount %>
    <%= form.number_field :amount, id: :material_reserve_has_armament_amount %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

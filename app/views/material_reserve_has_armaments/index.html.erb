<p id="notice"><%= notice %></p>

<h1>Material Reserve Has Armaments</h1>

<table>
  <thead>
    <tr>
      <th>Armaments</th>
      <th>Material reserve</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @material_reserve_has_armaments.each do |material_reserve_has_armament| %>
      <tr>
        <td><%= material_reserve_has_armament.armaments %></td>
        <td><%= material_reserve_has_armament.material_reserve %></td>
        <td><%= material_reserve_has_armament.amount %></td>
        <td><%= link_to 'Show', material_reserve_has_armament %></td>
        <td><%= link_to 'Edit', edit_material_reserve_has_armament_path(material_reserve_has_armament) %></td>
        <td><%= link_to 'Destroy', material_reserve_has_armament, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Material Reserve Has Armament', new_reserf_material_reserf_material_reserve_has_armament_url(@material_reserve) %>
